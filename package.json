{
  "name": "myone-web-functional",
  "version": "1.8.13",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-qa": "ng serve --configuration=qa",
    "start-dev": "ng serve --configuration=dev",
    "build": "ng build",
    "build-qa": "ng build --configuration=qa --buildOptimizer=true --commonChunk=true --optimization=true --outputHashing=all --vendorChunk=true  && node ./versioning.js qa",
    "build-dev": "ng build --configuration=dev && node ./versioning.js dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar": "sonar-scanner",
    "config-env": "node ./config/index.js",
    "myo-lint-test": "npm install && ng lint",
    "myo-build:dir": "npm run config-env && ng build --configuration production --base-href /myone-web-functional/",
    "myo-build:root": "npm run config-env && ng build --configuration production --base-href /",
    "myo-build:rh": "npm run config-env && ng build --aot --output-hashing=all --configuration production --base-href /",
    "myo-build:rhbv": "npm run config-env && ng build --aot --output-hashing=all --configuration production --build-optimizer --vendor-chunk --base-href /",
    "myo-deployment": "npm run myo-build:rhbv && node ./versioning.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@ng-bootstrap/ng-bootstrap": "^15.1.2",
    "@ng-select/ng-select": "^11.2.0",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/entity": "^16.2.0",
    "@ngrx/store": "^16.2.0",
    "@ngrx/store-devtools": "^16.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "^2.11.6",
    "@swiper/core": "^0.0.21",
    "@types/crypto-js": "^4.1.2",
    "angular-user-idle": "^4.0.0",
    "bootstrap": "^5.3.3",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.3",
    "detect-browser": "^5.3.0",
    "firebase": "^10.12.2",
    "i18n": "^0.15.1",
    "iconv-lite": "^0.6.3",
    "js-sha1": "^0.6.0",
    "js2xmlparser": "^5.0.0",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "moment": "^2.29.4",
    "ng-block-ui": "^4.0.1",
    "ng2-pdf-viewer": "^10.2.2",
    "ngrx-store-localstorage": "^16.0.0",
    "ngx-swiper-wrapper-hp": "^10.0.0",
    "ngx-toastr": "^16.1.0",
    "pace-js": "^1.2.3",
    "rxjs": "7.8.1",
    "rxjs-tslint": "^0.1.8",
    "simplebar": "^5.3.0",
    "simplebar-angular": "^3.2.4",
    "sweetalert2": "^11.6.13",
    "swiper": "^11.1.4",
    "tslib": "^2.6.3",
    "typings": "^2.1.1",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.12",
    "@types/bonjour": "^3.5.13",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "protractor": "~7.0.0",
    "sonar-scanner": "^3.1.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5"
  }
}
